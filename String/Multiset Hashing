https://dmoj.ca/submission/7384601

https://atcoder.jp/contests/abc367/tasks/abc367_f

#include <bits/stdc++.h>
using namespace std;
mt19937_64 rnd((unsigned int)chrono::steady_clock::now().time_since_epoch().count());
int64_t R(int64_t l, int64_t r) { return (l + (rnd() % (r - l + 1))); }
int32_t main() {
  cin.tie(0) -> sync_with_stdio(0);
  int n, q;
  cin >> n >> q;
  vector<int> a(n + 1, 0), b(n + 1, 0);
  vector<uint64_t> h(n + 1, 0);
  for (int i = 1; i <= n; i++) cin >> a[i];
  for (int i = 1; i <= n; i++) cin >> b[i];
  for (int i = 1; i <= n; i++) {
    h[i] = R(1 << 4, 1LL << 55);
  }
  vector<uint64_t> pref_a(n + 1, 0), pref_b(n + 1, 0);
  for (int i = 1; i <= n; i++) {
    pref_a[i] = pref_a[i - 1] + h[a[i]];
    pref_b[i] = pref_b[i - 1] + h[b[i]];
  }
  while (q--) {
    int l1, r1, l2, r2;
    cin >> l1 >> r1 >> l2 >> r2;
    if (r1 - l1 != r2 - l2) {
      cout << "No\n";
      continue;
    }
    uint64_t x = pref_a[r1] - pref_a[l1 - 1];
    uint64_t y = pref_b[r2] - pref_b[l2 - 1];
    cout << (x == y ? "Yes\n" : "No\n");
  }
  return 0;
}
