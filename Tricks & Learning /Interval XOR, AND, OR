#include <bits/stdc++.h>

using namespace std;

#ifndef ONLINE_JUDGE
#include "trace.cpp"
#else
#define dbg(...)
#endif

const int B = 60;

int64_t xor_0_n(int64_t n) {
  if (n % 4 == 0) return n;
  if (n % 4 == 1) return 1;
  if (n % 4 == 2) return n + 1;
  return 0;
}
int64_t xor_range(int64_t l, int64_t r) {
  return xor_0_n(r) ^ xor_0_n(l - 1);
}

int32_t main() {
  cin.tie(0) -> sync_with_stdio(0);
  int64_t l, r;
  cin >> l >> r;  
  int64_t OR = 0, AND = 0;
  for (int b = 0; b < B; b++) {
    if ((l >> b) == (r >> b)) {
      if (l >> b & 1) {
        AND |= 1LL << b;
        OR |= 1LL << b;
      }
    } else {
      OR |= 1LL << b;
    }
  }
  cout << xor_range(l, r) << " " << OR << " " << AND;
  return 0;
}
