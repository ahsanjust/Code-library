.stack 100h
.model small

.data
    msg1 db "Enter the first number: $"
    msg2 db "Enter the second number: $"
    msg3 db "Quotient: $"
    msg4 db "Remainder: $"
    Q db ?
    R db ?  
.code
    main proc
        mov ax, @data
        mov ds, ax
        
        lea dx, msg1
        mov ah, 9h
        int 21h
        
        mov ah, 1h
        int 21h      
        sub al, 48
        mov bl, al
        
        mov dl, 10
        mov ah, 2h
        int 21h
        mov dl, 13
        mov ah, 2h
        int 21h
        
        lea dx, msg2
        mov ah, 9h
        int 21h
        
        mov ah, 1h
        int 21h  
        sub al, 48
        mov bh, al
        
        mov al, bl
        mov ah, 0
        div bh   
        add al, '0'
        add ah, '0'
        mov Q, al  
        mov R, ah
        
        mov dl, 10
        mov ah, 2h
        int 21h
        mov dl, 13
        mov ah, 2h
        int 21h
        
        lea dx, msg3
        mov ah, 9h
        int 21h       
        
        mov dl, Q
        mov ah, 2h
        int 21h 
        
        mov dl, 10
        mov ah, 2h
        int 21h
        mov dl, 13
        mov ah, 2h
        int 21h
        
        lea dx, msg4
        mov ah, 9h
        int 21h       
        
        mov dl, R
        mov ah, 2h
        int 21h
        
        mov ah, 4ch
        int 21h
        
    endp main
end main
