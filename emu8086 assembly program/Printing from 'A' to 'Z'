.stack 100h
.model small

.data
    
    msg db "printing: $"

.code
    main proc
    mov ax, @data
    mov ds, ax
    
    mov cx, 26
    mov bl, 65
    start:
        lea dx, msg
        mov ah, 09h
        int 21h
        
        mov dl, bl
        mov ah, 02h
        int 21h
        
        mov dl, 10
        mov ah, 02h
        int 21h
        mov dl, 13
        mov ah, 02h
        int 21h
        
        inc bl 
            
    loop start
        
    mov ah, 4ch
    int 21h
    endp main
 end main
